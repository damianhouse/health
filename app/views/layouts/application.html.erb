<!DOCTYPE html>
<html>
  <head>
    <title>MyHealthStyle</title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="p:domain_verify" content="86506f23d5e55f6dbe1434256eb60afc"/>
    <%= favicon_link_tag 'favicon.ico' %>
    <script src="//load.sumome.com/" data-sumo-site-id="b086048ec9ea6218a358eab8f2cfa0e8dd277b5bd918e31755eb3e1f0522629c" async="async"></script>
  </head>
  <body class="healthstyle">
    <%= render 'layouts/nav_bar' %>
    <div class="content container-fluid" style="margin-top:40px;">
      <%= yield %>
    </div>
    <div class="text-center">
      <% if session[:user_id] %>
      <br \>
        <button class="btn sign-up-button"><%= link_to 'About Us', abouts_aboutus_path, class: "sign-up-text" %></button>
      <% end %>
    </div>
  </body>
  <script>
    window.fbAsyncInit = function() {
      FB.init({
        appId      : '545036969022775',
        xfbml      : true,
        version    : 'v2.7'
      });
    };

    (function(d, s, id){
       var js, fjs = d.getElementsByTagName(s)[0];
       if (d.getElementById(id)) {return;}
       js = d.createElement(s); js.id = id;
       js.src = "//connect.facebook.net/en_US/sdk.js";
       fjs.parentNode.insertBefore(js, fjs);
     }(document, 'script', 'facebook-jssdk'));
  </script>
</html>
